{
    "components" : {
        "dataio_0": {
            "module": "DataIO",
            "input": {
                "data": {
                    "data": [
                        "args.train_data"
                    ]
                }
            },
            "output": {
                "data": ["train"],
                "model": ["dataio"]
            }
         },
         "feature_scale_0": {
            "module": "FeatureScale",
            "input": {
                "data": {
                    "data": [
                        "dataio_0.train"
                    ]
                }
            },
            "output": {
                "data": ["train"],
                "model": ["feature_scale"]
            }
        },
      	"sampler_0":{
        	"module": "FederatedSample",
      		"input":{
      			"data": {
      				"data": [
      					"feature_scale_0.train"
      				]
      			}
      		},
      		"output": {
      			"data": ["train"],
      			"model": ["model"]
      		}
        } ,
        "dataio_1": {
            "module": "DataIO",
            "input": {
                "data": {
                    "data": [
                        "args.eval_data"
                    ]
                },
                "model": [
                    "dataio_0.dataio"
                ]
            },
            "output": {
                "data": ["eval_data"]
            }
        },
         "feature_scale_1": {
            "module": "FeatureScale",
            "input": {
                "data": {
                    "data": [
                        "dataio_1.eval_data"
                    ]
                },
                "model": [
                    "feature_scale_0.feature_scale"
                ]
            },
            "output": {
                "data": ["eval_data"],
                "model": ["feature_scale"]
            }
        },
        "homo_lr_0": {
            "module": "HomoLR",
            "input": {
                "data": {
                    "train_data": [
                        "sampler_0.train"
                    ]
                }
            },
            "output": {
                "data": ["train"],
                "model": ["aifel"]
            }
        },
		"homo_lr_1": {
            "module": "HomoLR",
            "input": {
                "data": {
                    "eval_data": [
                        "feature_scale_1.eval_data"
                    ]
                },
                "model": [
                    "homo_lr_0.aifel"
                ]
            },
            "output": {
                "data": ["predict"],
                "model": ["aifel"]
            }
        },
        "evaluation_0": {
            "module": "Evaluation",
            "input": {
                "data": {
                    "data": [
                        "homo_lr_0.train"
                    ]
                }
            },
            "output": {
                "data": ["evaluate"]
            }
        }, 
		"evaluation_1": {
            "module": "Evaluation",
            "input": {
                "data": {
                    "data": ["homo_lr_1.predict"]
                }
            },
            "output": {
                "data": ["evaluate"]
            }
        }
    }
}
