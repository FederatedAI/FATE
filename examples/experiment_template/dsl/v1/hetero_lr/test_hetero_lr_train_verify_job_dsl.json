{
    "components" : {
        "dataio_0": {
            "module": "DataIO",
            "input": {
                "data": {
                    "data": ["args.train_data"]
                }
            },
            "output": {
                "data": ["train"],
                "model": ["dataio"]
            },
			"need_deploy": true
         },
        "dataio_1": {
            "module": "DataIO",
            "input": {
                "data": { "data": ["args.eval_data"] },
                "model":["dataio_0.dataio"]
            },
            "output": {
                "data": ["eval"],
                "model": ["dataio"]
            },
			"need_deploy": true
         },
		 "dataio_2": {
            "module": "DataIO",
            "input": {
                "data": { "data": ["args.test_data"] },
                "model":["dataio_1.dataio"]
            },
            "output": {
                "data": ["test"],
                "model": ["dataio"]
            },
			"need_deploy": true
         },
        "intersection_0": {
             "module": "Intersection",
             "input": {
                 "data": {
                     "data": ["dataio_0.train"]
                 }
             },
             "output": {
                 "data": ["train"]
             }
         },
        "intersection_1": {
             "module": "Intersection",
             "input": {
                 "data": {
                     "data": ["dataio_1.eval"]
                 }
             },
             "output": {
                 "data": ["eval"]
             }
         },
		 "intersection_2": {
             "module": "Intersection",
             "input": {
                 "data": {
                     "data": ["dataio_2.test"]
                 }
             },
             "output": {
                 "data": ["test"]
             }
         },
        "hetero_feature_binning_0": {
            "module": "HeteroFeatureBinning",
            "input": {
                "data": {
                    "data": ["intersection_0.train"]
                }
            },
            "output": {
                "data": ["transform_data"],
                "model": ["binning_model"]
            },
            "need_deploy": false
        },
        "statistic_0": {
            "module": "DataStatistics",
            "input": {
                "data": {
                    "data": [
                        "intersection_0.train"
                    ]
                }
            },
            "output": {
                "data": ["train_data"],
                "model": ["model"]
            },
            "need_deploy": false
        },
        "hetero_feature_selection_0": {
            "module": "HeteroFeatureSelection",
            "input": {
                "data": {
                    "data": ["intersection_0.train"]
                },
                "isometric_model": ["hetero_feature_binning_0.binning_model",
				    "statistic_0.model"]
            },
            "output": {
                "data": ["train"],
                "model": ["selected"]
            }
        },
        "hetero_feature_selection_1": {
            "module": "HeteroFeatureSelection",
            "input": {
                "data": {
                    "data": ["intersection_1.eval"]
                },
                "model": ["hetero_feature_selection_0.selected"]
            },
            "output": {
                "data": ["eval"],
                "model": ["selected"]
            }
        },
		"hetero_feature_selection_2": {
            "module": "HeteroFeatureSelection",
            "input": {
                "data": {
                    "data": ["intersection_2.test"]
                },
                "model": ["hetero_feature_selection_1.selected"]
            },
            "output": {
                "data": ["test"],
                "model": ["selected"]
            }
        },
        "feature_scale_0": {
            "module": "FeatureScale",
            "input": {
                "data": {
                    "data": ["hetero_feature_selection_0.train"]
                }
            },
            "output": {
                "data": ["train"],
                "model": ["feature_scale"]
            }
        },
        "feature_scale_1": {
            "module": "FeatureScale",
            "input": {
                "data": {
                    "data": ["hetero_feature_selection_1.eval"]
                },
                "model":["feature_scale_0.feature_scale"]
            },
            "output": {
                "data": ["eval"],
                "model": ["feature_scale"]
            }
        },
		"feature_scale_2": {
            "module": "FeatureScale",
            "input": {
                "data": {
                    "data": ["hetero_feature_selection_2.test"]
                },
                "model":["feature_scale_1.feature_scale"]
            },
            "output": {
                "data": ["test"],
                "model": ["feature_scale"]
            }
        },
        "hetero_lr_0": {
            "module": "HeteroLR",
            "input": {
                "data": {
                    "train_data": ["feature_scale_0.train"],
                    "eval_data": ["feature_scale_1.eval",
                                  "feature_scale_2.test"]
                }
            },
            "output": {
                "data": ["train"],
                "model": ["hetero_lr"]
            }
        },
        "evaluation_0": {
            "module": "Evaluation",
            "input": {
                "data": {
                    "data": ["hetero_lr_0.train"]
                }
            }
        }
    }
}
